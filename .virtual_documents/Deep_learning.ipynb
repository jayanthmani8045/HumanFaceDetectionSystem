df_clean['bbox_width'] = df_clean['x1'] - df_clean['x0']
df_clean['bbox_height'] = df_clean['y1'] - df_clean['y0']
df_clean['bbox_area'] = df_clean['bbox_width'] * df_clean['bbox_height']

